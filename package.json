{
  "name": "server",
  "version": "0.0.1",
  "description": "reusable server",
  "main": "index.js",
  "author": "Marc Hurabielle",
  "license": "MIT",
  "scripts": {
    "prod": "rm -fr serve_prod/build && node_modules/.bin/webpack --config webpack_prod.config.js && cd serve_prod/build && node backend.bundle.js",
    "build": "rm -fr serve_prod/build && node_modules/.bin/webpack --config webpack_prod.config.js",
    "docker-image": "npm run build && cd serve_prod && docker build -t aywan/node-web-app .",
    "docker-start": "docker run -p 4200:4200 -d aywan/node-web-app",
    "docker": "npm run docker-image && npm run docker-start",
    "dev": "node_modules/.bin/webpack --config webpack_dev_server.config.js && node dev_server.bundle.js",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "modulePaths": [
      "frontend/src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "frontend/src",
      "node_modules"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "jest": "^20.0.0",
    "node-sass": "^4.5.0",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "bulma": "^0.4.1",
    "ejs": "^2.5.6",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-form": "^6.6.3",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "redux-saga": "^0.14.3",
    "rxjs": "^5.3.0",
    "style-loader": "^0.15.0"
  }
}
